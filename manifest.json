{
  "manifest_version": 3,
  "name": "BazaarDBKR",
  "description": "BazaarDB의 이름 한글화 크롬 확장 기능입니다.",
  "version": "1.4",
  "action": {
    "default_popup": "/popup/popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.bazaardb.gg/*"
      ],
      "js": [
        "data.js",
        "const.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "https://*.bazaardb.gg/*"
  ],
  "icons": {
    "128": "icon128.png"
  }
}
